from logger import input_data , print_data

def interface():
    print("Здарова !\n 1-запись данных \n 2-вывод данных")
    command = int(input("print number "))
    while command !=1 and command !=2 :
        print( " Введите коректные данные ")
        command = int(input("print number "))
    if command == 1  :
        input_data()
    elif command == 2 :
        print_data()
        
